{
  "AWSEBDockerrunVersion": "2",
  "Authentication": {
    "Bucket": "mwaside",
    "Key": ".dockercfg"
  },
  "containerDefinitions": [
  {
      "name": "loggy",
      "image": "sendgridlabs/loggly-docker",
      "memory": 128,
      "environment": [
        {
          "name": "TOKEN",
          "value": "dddaaa51-f410-4d6e-ae03-3a7fe250e561"
        },
        {
          "name":"TAG",
          "value": "Docker"
        }
      ]
    },
    {
      "name": "nginx",
      "image": "quay.io/jumo/sms_nginx:0.0.2",
      "essential": true,
      "memory": 128,
      "links": ["web"],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
    },
    {
      "name": "web",
      "image": "mwaaas/watu:<TAG>",
      "essential": true,
      "links": [
        "loggy"
      ],
      "memory": 128,
      "command": ["./app_server.sh"],
        "portMappings": [
        {
          "hostPort": 8000,
          "containerPort": 3000
        }
      ]
    }
  ]
}